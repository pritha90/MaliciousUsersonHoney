{
    "manifest_version": 2,
    "name": "Test Honey",
    "version": "1",
    "description": "Testing all Honey Coupons",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "background": {
        "scripts": [
            "b.js"
        ],
    "persistent": false
    },    
    "web_accessible_resources": [
        "images/*"
    ],
    "permissions": [
        "tab",
        "activeTab",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "icons/logo.png",
        "48": "icons/logo.png",
        "128": "icons/logo.png"
    },
    "browser_action": {
    "default_icon": "icons/logo.png"
  }
}
